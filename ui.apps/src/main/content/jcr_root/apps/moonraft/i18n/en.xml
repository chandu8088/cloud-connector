<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:language="en"
    jcr:mixinTypes="[mix:language]"
    jcr:primaryType="nt:folder">
    <internalerror
        jcr:primaryType="sling:MessageEntry"
        sling:key="internalError"
        sling:message="Internal Error occurred please try again after some time"/>
    <nocontent
        jcr:primaryType="sling:MessageEntry"
        sling:key="noContent"
        sling:message="No Files or Folder found"/>
    <valueerror
        jcr:primaryType="sling:MessageEntry"
        sling:key="valueError"
        sling:message="Please configure the folder Id to view Result"/>
</jcr:root>
